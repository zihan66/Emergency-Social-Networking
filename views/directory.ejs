<link rel="stylesheet" href="/stylesheets/directory.css" />
<div class="directory-wrapper">
  <header class="directory-header">
    <!-- <span class="header1">User Directory</span> -->
    <i class="bars icon" id="bar"></i>
    <button id="go-to" class="ui inverted button compact">
      See Public Messages
    </button>
  </header>
  <div class="hamburger-menu">
    <div class="links">
      <div class="menu-item">
        <button class="ui inverted whitesmoke button" id="status-bar">
          My Status:
        </button>
      </div>
      <div class="links2">
        <!-- <div class="ham-img">
        </div> -->
        <p></p>
        <form class="ui form">
          <button id="setRedButton" class="ui inverted button compact">
            <img
              class="ui rounded-image"
              height="30"
              width="30"
              src="/images/red.png"
            />
            <i> Emergency </i>
            <i class="check circle outline icon"></i>
          </button>
          <button id="setGreenButton" class="ui inverted button compact">
            <img
              class="ui rounded-image"
              height="30"
              width="30"
              src="/images/green.png"
            />
            <i> I am OK </i>
            <i class="circle outline icon"></i>
          </button>
          <p></p>
          <button id="setYellowButton" class="ui inverted button compact">
            <img
              class="ui rounded-image"
              height="30"
              width="30"
              src="/images/yellow.png"
            />
            <i> I need Help </i>
            <i class="circle outline icon"></i>
          </button>
          <button id="setGreyButton" class="ui inverted button compact">
            <img
              class="ui rounded-image"
              height="30"
              width="30"
              src="/images/grey.png"
            />
            <i> Undefined </i>
            <i class="circle outline icon"></i>
          </button>
        </form>

        <!-- <button class="ui inverted whitesmoke button" id="status-link1">
          Lol1
        </button>
        <button class="ui inverted whitesmoke button" id="status-link2">
          Lol2
        </button>
        <button class="ui inverted whitesmoke button" id="status-link3">
          Lol3
        </button>
        <button class="ui inverted whitesmoke button" id="status-link4">
          Lol4
        </button> -->
      </div>
      <HR WIDTH="90%" SIZE="7" ALIGN="middle" NOSHADE />
      <div class="menu-item">
        <button class="ui inverted whitesmoke button" id="link-signup">
          This is a link.
        </button>
      </div>
      <HR WIDTH="90%" SIZE="7" ALIGN="middle" NOSHADE />
      <button id="logout" class="ui inverted button compact">Logout</button>
      <HR WIDTH="90%" SIZE="7" ALIGN="middle" NOSHADE />
    </div>
  </div>
  <div class="directory-container">
    <ul class="user-list"></ul>
  </div>
  <script src="https://cdn.jsdelivr.net/npm/brownies"></script>
  <script src="/socket.io/socket.io.js"></script>
  <script src="/javascripts/directory.js"></script>
</div>
